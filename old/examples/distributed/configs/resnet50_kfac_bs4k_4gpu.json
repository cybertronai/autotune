{
  "dataset": "ImageNet",
  "epochs": 36,
  "batch_size": 32,
  "val_batch_size": 32,
  "random_resized_crop": true,
  "random_horizontal_flip": true,
  "normalizing_data": true,
  "arch_file": "models/resnet_b.py",
  "arch_name": "resnet50",
  "arch_args": {
    "zero_init_residual": false,
    "norm_stat_momentum": 0.9
  },
  "optim_name": "DistributedSecondOrderOptimizer",
  "optim_args": {
    "curv_type": "Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "lr": 8.18e-3,
    "momentum": 0.997,
    "momentum_type": "preconditioned",
    "normalizing_weights": true,
    "non_reg_for_bn": true,
    "acc_steps": 32
  },
  "curv_args": {
    "damping": 2.5e-4,
    "ema_decay": 1,
    "pi_type": "tracenorm"
  },
  "momentum_correction": true,
  "scheduler_name": "PolynomialDecayIterLR",
  "scheduler_args": {
    "start_iter": 10016,
    "rate": 11,
    "max_count": 531200
  }
}
