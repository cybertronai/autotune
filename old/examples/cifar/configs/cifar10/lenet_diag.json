{
  "dataset": "CIFAR-10",
  "epochs": 100,
  "batch_size": 128,
  "val_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": true,
  "arch_file": "models/lenet.py",
  "arch_name": "LeNet5",
  "optim_name": "SecondOrderOptimizer",
  "optim_args": {
    "curv_type":"DiagFisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron"
    },
    "momentum": 0.9,
    "momentum_type": "grad",
    "adjust_momentum": true,
    "weight_decay": 0.05
  },
  "curv_args": {
    "damping": 1e-4,
    "ema_decay": 0.99
  },
  "lr": 0.01,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 0.9
  },
  "no_cuda": false
}