{
  "dataset": "MNIST",
  "epochs": 150000,
  "batch_size": 60000,
  "test_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": false,
  "arch_file": "models/mlp_autoencoder_deep.py",
  "arch_name": "MLP_AUTOENCODER_DEEP",
  "optim_name": "SecondOrderOptimizer",
  "optim_args": {
    "lr": 0.01,
    "curv_type":"Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "momentum": 0.9,
    "momentum_type": "preconditioned",
    "weight_decay": 0
  },
  "curv_args": {
    "damping": 3e-3,
    "ema_decay": 1
  },

  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 1.0
  },
  "log_interval": 1,
  "no_cuda": false
}