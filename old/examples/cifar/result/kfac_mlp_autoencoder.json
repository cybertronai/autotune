{
  "dataset": "MNIST",
  "epochs": 1500000,
  "batch_size": 60000,
  "test_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": false,
  "arch_file": "models/mlp_autoencoder.py",
  "arch_name": "MLP_AUTOENCODER",
  "optim_name": "SecondOrderOptimizer",
  "optim_args": {
    "curv_type":"Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "momentum": 0.9,
    "momentum_type": "preconditioned",
    "weight_decay": 0.05
  },
  "curv_args": {
    "damping": 1e-4,
    "ema_decay": 0.333
  },

  "lr": 0.2,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 1.0
  },
  "log_interval": 1,
  "no_cuda": false
}