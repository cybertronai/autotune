{
  "epochs": 90,
  "batch_size": 32,
  "val_batch_size": 128,
  "random_crop": true,
  "random_horizontal_flip": true,
  "normalizing_data": true,
  "arch_name": "resnet50",
  "optim_name": "DistributedSecondOrderOptimizer",
  "optim_args": {
    "curv_type": "Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "momentum": 0.9,
    "momentum_type": "preconditioned",
    "adjust_momentum": true,
    "weight_decay": 0.05,
    "acc_steps": 4
  },
  "curv_args": {
    "damping": 1e-4,
    "ema_decay": 0.333,
    "pi_type": "tracenorm"
  },
  "lr": 0.04,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 0.9
  },
  "log_interval": 64,
  "no_cuda": false
}
