{
  "dataset": "ImageNet",
  "epochs": 90,
  "batch_size": 32,
  "val_batch_size": 32,
  "random_resized_crop": false,
  "random_crop": true,
  "random_horizontal_flip": true,
  "normalizing_data": true,
  "arch_file": "models/resnet_b.py",
  "arch_name": "resnet50",
  "arch_args": {
    "zero_init_residual": true,
    "norm_stat_momentum": 0.1
  },
  "optim_name": "DistributedSecondOrderOptimizer",
  "optim_args": {
    "curv_type": "Fisher",
    "curv_shapes": {
      "Conv2d": "Diag",
      "Linear": "Diag",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "lr": 1.6,
    "momentum": 0.9,
    "momentum_type": "raw",
    "non_reg_for_bn": true
  },
  "curv_args": {
    "damping": 2.5e-4,
    "ema_decay": 0.999
  },
  "momentum_correction": true,
  "scheduler_name": "MultiStepLR",
  "scheduler_args": {
    "milestones": [30, 60, 80],
    "gamma": 0.1
  },
  "warmup_epochs": 5,
  "warmup_scheduler_name": "GradualWarmupIterLR",
  "warmup_scheduler_args": {
    "initial_lr": 0.0125,
    "max_count": 1565
  }
}
