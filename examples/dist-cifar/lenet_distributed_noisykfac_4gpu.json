{
  "dataset": "CIFAR-10",
  "epochs": 15,
  "batch_size": 64,
  "test_batch_size": 128,
  "num_mc_sample_group": 4,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": false,
  "arch_file": "models/lenet.py",
  "arch_name": "LeNet5",
  "optim_name": "DistributedVIOptimizer",
  "optim_args": {
    "curv_type": "Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "momentum": 0.9,
    "adjust_momentum": false,
    "momentum_type": "preconditioned",
    "weight_decay": 0.1,
    "num_mc_samples": 1,
    "test_num_mc_samples": 0,
    "kl_weighting": 0.2,
    "prior_variance": 1,
    "acc_steps": 1
  },
  "curv_args": {
    "damping": 1e-4,
    "ema_decay": 0.333,
    "pi_type": "tracenorm"
  },
  "lr": 0.004,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 0.9
  },
  "log_interval": 64,
  "no_cuda": false
}
