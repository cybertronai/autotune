{
  "dataset": "CIFAR-10",
  "epochs": 15,
  "batch_size": 32,
  "test_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": true,
  "arch_file": "models/lenet.py",
  "arch_name": "LeNet5",
  "optim_name": "DistributedSecondOrderOptimizer",
  "optim_args": {
    "curv_type": "Fisher",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "momentum": 0.9,
    "momentum_type": "preconditioned",
    "adjust_momentum": true,
    "weight_decay": 0.05,
    "acc_steps": 1
  },
  "curv_args": {
    "damping": 1e-4,
    "ema_decay": 0.333,
    "pi_type": "tracenorm"
  },
  "lr": 0.04,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 0.9
  },
  "log_interval": 64,
  "no_cuda": false
}
