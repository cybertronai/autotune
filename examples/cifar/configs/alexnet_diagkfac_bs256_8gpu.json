{
    "dataset": "CIFAR-10",
    "epochs": 161,
    "batch_size": 32,
    "val_batch_size": 128,
    "random_crop": false,
    "random_horizontal_flip": false,
    "normalizing_data": true,
    "arch_file": "models/alexnet.py",
    "arch_name": "AlexNet",
    "optim_name": "DistributedSecondOrderOptimizer",
    "optim_args": {
        "curv_type": "Fisher",
        "curv_shapes": {
            "Conv2d": "Diag",
            "Linear": "Kron",
            "BatchNorm1d": "Diag",
            "BatchNorm2d": "Diag"
        },
        "lr": 1e-4,
        "l2_reg": 1e-4,
        "momentum": 0.9,
        "momentum_type": "raw"
    },
    "curv_args": {
        "damping": 1e-4,
        "ema_decay": 0.666,
        "pi_type": "tracenorm"
    },
    "scheduler_name": "MultiStepLR",
    "scheduler_args": {
        "milestones": [80, 120],
        "gamma": 0.1
    }
}