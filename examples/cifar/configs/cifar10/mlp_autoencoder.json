{
  "dataset": "MNIST",
  "epochs": 15,
  "batch_size": 10000,
  "test_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": false,
  "arch_file": "models/mlp_autoencoder.py",
  "arch_name": "MLP_AUTOENCODER",
  "optim_name": "Adam",
  "optim_args": {
    "betas": [0.9, 0.999],
    "weight_decay": 0.01
  },
  "lr": 0.2,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 1.0
  },
  "log_interval": 1,
  "no_cuda": false
}