{
    "dataset": "CIFAR-10",
    "epochs": 10,
    "batch_size": 64,
    "test_batch_size": 128,
    "random_crop": false,
    "random_horizontal_flip": false,
    "normalizing_data": true,
    "arch_file": "models/lenet.py",
    "arch_name": "LeNet5",
    "optim_name": "SecondOrderOptimizer",
    "optim_args": {
        "curv_type":"KronFisher",
        "adjust_momentum": true,
        "damping": 1e-7,
        "ema_decay": 0.333,
        "weight_decay": 222
    },
    "lr": 9e-6,
    "no_cuda": false
}