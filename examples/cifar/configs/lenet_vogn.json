{
  "dataset": "CIFAR-10",
  "epochs": 30,
  "batch_size": 128,
  "test_batch_size": 128,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": true,
  "arch_file": "models/lenet.py",
  "arch_name": "LeNet5BatchNorm",
  "arch_args": {
    "affine": false
  },
  "optim_name": "VIOptimizer",
  "optim_args": {
    "curv_type":"DiagFisher",
    "momentum": 0.9,
    "adjust_momentum": false,
    "momentum_type": "precgrad",
    "weight_decay": 0,
    "num_mc_samples": 10,
    "kl_weighting": 1,
    "prior_variance": 1
  },
  "curv_args": {
    "damping": 2e-5,
    "ema_decay": 0.001
  },
  "lr": 0.01,
  "scheduler_name": "ExponentialLR",
  "scheduler_args": {
    "gamma": 0.9
  },
  "no_cuda": false
}
